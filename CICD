# private docker registry

docker run -d -p 5000:5000 -v E:/localhub/registry:/var/lib/registry --restart=unless-stopped --name hub.local registry


docker run -it -p 5001:5001 --name registry-web --link hub.local -e REGISTRY_URL=http://hub.local:5000/v2 -e REGISTRY_NAME=localhost:5000 hyper/docker-registry-web